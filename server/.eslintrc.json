{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "eslint-config-prettier", "airbnb-base"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": 0,
        "no-console": 0,
        "arrow-parens": 0,
        "object-curly-spacing": 0,
        "indent": [0, 4],
        "no-tabs": 0,
        "implicit-arrow-linebreak": 0
    }
}
